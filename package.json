{
  "name": "server",
  "version": "0.0.0",
  "description": "description",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently \"yarn graphql-code\"  \"NODE_ENV=dev nodemon\"  \" yarn graphql-codegen --config codegen.yml --watch \"  ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "node script/e2eSetting.js && NODE_ENV=dev_test jest",
    "test:watch": "jest --watch",
    "test:cov": "node script/e2eSetting.js && NODE_ENV=dev_test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "graphql-code": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "type-graphql": "^0.17.1",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.9",
    "@graphql-codegen/cli": "^1.0.6",
    "@graphql-codegen/fragment-matcher": "^1.0.6",
    "@graphql-codegen/typescript": "1.0.6",
    "@graphql-codegen/typescript-resolvers": "1.0.6",
    "@graphql-inspector/cli": "^1.14.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/graphql": "^6.0.1",
    "@nestjs/platform-express": "^6.0.1",
    "@nestjs/testing": "^6.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "@types/express": "^4.16.0",
    "@types/faker": "^4.1.5",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.123",
    "@types/mysql": "^2.15.5",
    "@types/node": "^11.13.0",
    "@types/ramda": "^0.26.5",
    "@types/supertest": "^2.0.5",
    "add": "^2.0.6",
    "apollo-boost": "^0.3.1",
    "apollo-server-express": "^2.4.8",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "concurrently": "^4.1.0",
    "faker": "^4.1.0",
    "graphql": "^14.1.1",
    "graphql-codegen-add": "^0.18.0",
    "graphql-codegen-time": "^0.18.0",
    "graphql-codegen-typescript-common": "^0.18.0",
    "graphql-codegen-typescript-resolvers": "^0.18.0",
    "graphql-codegen-typescript-server": "^0.18.0",
    "graphql-tools": "^4.0.4",
    "graphql-voyager": "^1.0.0-rc.26",
    "http-server": "^0.11.1",
    "jest": "^23.5.0",
    "lodash": "^4.17.11",
    "mysql": "^2.16.0",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "ramda": "^0.26.1",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "typeorm": "^0.2.15"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$|.e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../doc/coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
